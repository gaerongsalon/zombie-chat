{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "interface-over-type-literal": false,
    "max-classes-per-file": false
  },
  "linterOptions": {
    "exclude": ["node_modules", ".webpack", "webpack.config.js"]
  }
}
